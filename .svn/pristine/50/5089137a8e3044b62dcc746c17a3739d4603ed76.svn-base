/**
 *
 * @author Jacob Christoffers
 */
public class OperatingSystem 
{
    //instance variables

    /**
     * name
     */
    public String name;

    /**
     * release year
     */
    public int releaseDate;

    /**
     * cost
     */
    public int cost;

    /**
     * use
     */
    public String use;
    
    //constructor
    /**
     * constructor
     */
    public OperatingSystem()
    {
        this.name = name;
        this.releaseDate = releaseDate;
        this.cost = cost;
        this.use = use;
    }
    
    //get methods
    /**
     * get name
     * @return name
     */
    public String getName()
    {
        return name;
    }

    /**
     * get release year
     * @return release date
     */
    public int getReleaseDate()
    {
        return releaseDate;
    }

    /**
     * get cost
     * @return cost
     */
    public int getCost()
    {
        return cost;
    }

    /**
     * get use
     * @return use
     */
    public String getUse()
    {
        return use;
    }
    
    //set methods
    /**
     * set new name
     * @param name name
     * @return new name
     */
    public String setName(String name)
    {
        return this.name = name;
    }

    /**
     * set new release date
     * @param releaseDate release date
     * @return new release date
     */
    public int setReleaseDate(int releaseDate)
    {
        return this.releaseDate;
    }

    /**
     * set new cost
     * @param cost cost
     * @return new cost
     */
    public int setCost(int cost)
    {
        return this.cost;
    }

    /**
     * set new use
     * @param use use
     * @return new use
     */
    public String setUse(String use)
    {
        return this.use;
    }
    
    //toString
    @Override
    public String toString()
    {
        return "OS Name: " +getName() +"\nRelease Date: " +getReleaseDate() + "\nCost: $" +getCost() +
                "\nBest Features/Use: " +getUse();
    }
}
